@model ResturantReviewApp.Models.Restaurant

<h1>@Model.Name</h1>
<p><strong>Location:</strong> @Model.Location</p>
<p><strong>Cuisine:</strong> @Model.CuisineType</p>
<p><strong>Rating:</strong> @Model.Rating</p>
<p><strong>Reviews:</strong> @string.Join(", ", Model.Reviews)</p> <!-- Include this only if Reviews is in the model -->
<a asp-action="Index" class="btn btn-secondary">Back to List</a>

